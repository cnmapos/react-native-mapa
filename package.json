    "git-cz": "^4.9.0",
    "husky": "^9.0.11",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-docgen-typescript": "^2.2.2",
    "react-test-renderer": "18.2.0",
    "typedoc-plugin-markdown": "^4.0.0-next.53",
